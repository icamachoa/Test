	function CorreoEnviado(destinatario) {

		var correo = "enviacorreo";
		var methods = {
			enviacorreo : function() {
				$.fallr('show', {
					content : '<p><b>El correo a: ' + destinatario + ' </br> ha sido enviado.</b></p>',
					width : '400px',
					height : '150px',
					autoclose : 3000,
					icon : 'info',
					closeKey : true,
					position : 'center'
				});
			}
		};

		setTimeout(function() {
			methods[correo].apply(this, [destinatario]);
		}, 1000);
	}

