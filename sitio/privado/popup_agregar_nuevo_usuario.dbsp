    SalesUp.Variables.ValidaSoloNumeros=function(Op){

      var v = $.trim(Op.v);
      var $Elemento = $(Op.t);
      var $DentroDe = $Elemento.closest('form');
      var numeros=/^[-|+]?([0-9])*[1,2]*$/;
      var resultado=numeros.test(v);
      if(!resultado){
                SalesUp.Construye.MuestraMsj({tMsg:4, Destino:$DentroDe, Msg:'Solo se permite ingresar números, por favor verifique de nuevo.' });
                SalesUp.Valida.MarcarObligatorio($Elemento);
                SalesUp.Valida.FocusMal();
            }
            
              return resultado;
    }

    SalesUp.Variables.ValidarTamanioContasenia=function(Op){
       $('#repass').val('');
        var $Elemento=$(Op.t);
        var $DentroDe = $Elemento.closest('form');
        var tamaniopasswd=$Elemento.val();

        if(tamaniopasswd==''){return;}
         if(tamaniopasswd.length>=8){
            tamanio=true;
            
          }else{
            tamanio= false;
             SalesUp.Construye.MuestraMsj({tMsg:4, Destino:$DentroDe, Msg:'La contraseña debe ser al menos de 8 dígitos' });
