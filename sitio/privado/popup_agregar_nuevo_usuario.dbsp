    SalesUp.Variables.TamanioIncorrecto=function(){
      $('#pass').focusout(function (){
        var cambiarPswActivo=$('#Cambiarcontrasena').attr('data-activo');
        var tam=$('#pass').val();
        tam=tam.length;

        if(tam==0){
          return true;
          } else if((tam<8)  &&(cambiarPswActivo==1)){
                SalesUp.Construye.MuestraMsj({tMsg:4, Destino:'#frmAgregarUsuario', Msg:'La contraseña debe tener al menos 8 caracteres.' });
                 $('#pass').focus();
                return false; 
              }
           });
      return true;
    }
     var array = [];
     var cambiarPswActivo;
