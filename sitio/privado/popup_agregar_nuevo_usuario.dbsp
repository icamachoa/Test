                  return false;
                }else{
                  setTimeout(function() { $('#spin').addClass('fa-check Verde').removeClass('fa-times oculto fa-spin fa-spinner');}, 200);
                 //Aqui ando
                  return true;
             
                }
            }
            /*}, 100);*/
    }
