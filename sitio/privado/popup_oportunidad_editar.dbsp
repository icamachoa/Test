        <#include doc="/privado/CrearEventoNuevo.dbsp"/>
        <#KILLCOOKIE NAME="ESTAVENCIDO"/>
        <#killCOOKIE NAME="ESCLIENTE"/>  
        <#killCOOKIE NAME="ESOPORTUNIDAD"/>  
        
      <input type="hidden" id="PlantillaSeleccionada" name="PlantillaSeleccionada"/>
