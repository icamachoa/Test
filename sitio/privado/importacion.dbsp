  	$('.ayudaGoogle .slidesjs-pagination-item > a').click(function(){ 
  		var iditem = $(this).attr('data-slidesjs-item'); 
  		if(iditem == 0){
  			$('.textoAyudaGoogle').html('1. Dentro de tu correo Gmail dirígete al menú localizado en la parte superior izquierda de tu pantalla y selecciona <b>Contactos</b>');
