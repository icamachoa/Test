      <div id="ListaFiltros">
        <div id="filtros">
          
        </div>
      </div>
      <div id="tablaDatos" class="contenedor-datos"></div>
      <div class="w100">
        <p>* Si el ejecutivo no está listado aquí, significa que no ha tenido actividad alguna en el periodo.</p>
      </div>
      <!--<#IF EXPRESSION="SESSION.PUEDEEXPORTAR>0">
        <div class="acciones visualizar">
          <a id="btnexportar" class="boton exportar">Exportar Reporte</a>
          <input type="hidden" name="titulo" id="titulo" value="Actividades_Ejecutivos" />
        </div>
      <#/IF>-->
    </div>
