  resultado = OpenPay.card.validateCardNumber($("#TARJETA_NUMERO").val());
  
  if (!resultado){
    SalesUp.Construye.MuestraMsj({Destino:"#FrmTipoPago",tMsg:4, Msg:'La tarjeta es inválida.', Tiempo:4000});
    return false;
  }
  resultado = OpenPay.card.validateCVC($("#TARJETA_CODIGO").val());
  if (!resultado){
    SalesUp.Construye.MuestraMsj({Destino:"#FrmTipoPago",tMsg:4, Msg:'El código de seguridad es inválido.', Tiempo:4000});
    return false;
  }
  resultado = OpenPay.card.validateExpiry($("#TARJETA_MES").val(),'20'+$("#TARJETA_ANNIO").val());
  if (!resultado){
    SalesUp.Construye.MuestraMsj({Destino:"#FrmTipoPago",tMsg:4, Msg:'La expiración de la tarjeta es incorrecta.', Tiempo:4000});
    return false;
  }
  return resultado; 
}

SalesUp.Variables.CambiarDatosTarjeta=function(Op){
  var v=(Op.v)?Op.v:''; 
  var t=(Op.t)?Op.t:''; 
  var id=$(t).attr('id');
  if(id=='TARJETA_CODIGO'){
    $('#frm_t_c').val(v);
  }else if(id=='TARJETA_NUMERO'){
    $('#frm_t_num').val(v);
  }else if(id=='TARJETA_NOMBRE'){
    $('#frm_t_nom').val(v);
  }else if(id=="TARJETA_TIPO"){
    $('#frm_t_t').val(v);
  }else if(id=='TARJETA_ANNIO'){
    $('#frm_t_a').val(v);
  }else if(id=='TARJETA_MES'){
    $('#frm_t_mes').val(v);
  }

}



SalesUp.Variables.MostrarTextoPrecio =function(Op){
   var precio=(Op.precio)?parseFloat(Op.precio).toFixed(2):'149';
   var paisDef=$('#paisDefault').val();
   textoMxn='$ <span class="precio">'+precio+'</span> MXN (Peso mexicano) por usuario';
   textoUsd='$ <span class="precio">'+precio+'</span> USD (Dólar estadounidense) por usuario';
   var txt=(paisDef=='MX')? textoMxn : textoUsd;
   $('.InformacionPrecio').html(txt);
}
SalesUp.Variables.MostrarInformacionPago=function(Op){
   var v=(Op.v)?Op.v:'';
   var t=(Op.t)?Op.t:'';
   if(v==1){
    $('#PagoPaypal').hide('slow');
    $('#PagoDeposito').hide('slow');
   }else if(v==2){
    $('#PagoPaypal').show('slow');
    $('#PagoDeposito').hide('slow');
   }else if(v==3){
    $('#PagoPaypal').hide('slow');
    $('#PagoDeposito').show('slow');
   }
   SalesUp.Variables.establecrFormaPago({v:v});
}
