SalesUp.Variables.MostrarTipoPrecio=function(){
  var defaultPais=$('#paisDefault').val();
  var precioBasico=$('#Licencia1'); 
  var precioPro=$('#Licencia2');
  var idLicencia=$('#idLicencia').val();
  var pPro=(defaultPais=='MX')? $(precioPro).attr('data-preciomx') : $(precioPro).attr('data-preciousd');
  var pBasico=(defaultPais=='MX')? $(precioBasico).attr('data-preciomx') : $(precioBasico).attr('data-preciousd');
  var precio=(idLicencia==1)? pBasico: pPro; 
  var textoMxn='$ <span class="precio">'+precio+'</span> MXN (Peso mexicano) por usuario';
  var textoUsd='$ <span class="precio">'+precio+'</span> USD (Dólar estadounidense) por usuario';
  (defaultPais=='MX') ? $('.InformacionPrecio').html(textoMxn) : $('.InformacionPrecio').html(textoUsd);
  (defaultPais=='MX') ? $('.TipoMoneda').html('MXN'): $('.TipoMoneda').html('USD');
  $('#LicenciaId'+idLicencia).attr('checked', true);
  $("#NumUsuarios").spin({max:100,min:1,buttonUp:SalesUp.Variables.CalcularLicenciaSalesUp,buttonDown:SalesUp.Variables.CalcularLicenciaSalesUp});
    
}
SalesUp.Variables.Operacion=function(Op){
  var valor=(Op.v)?Op.v:''; 
  var elemento=(Op.t)?Op.t:'';

  if(valor==0){
    $('.preciosLicencias, .eligeUsuarios, .PasoCompra.frecuencia').removeClass('Inactivo');
  }else if(valor==1){
    $('.preciosLicencias').addClass('Inactivo');
    $('.eligeUsuarios, .PasoCompra.frecuencia').removeClass('Inactivo');
    var idLicencia=$('#idLicencia').val();
    var idFrec=$('#frecuenciaAct').val();
    $('#LicenciaId'+idLicencia).attr('checked', true);
    $('.frecuencialicencia[value='+idFrec+']').attr('checked', true);
  }else if(valor==2){
    $('.preciosLicencias, .eligeUsuarios, .PasoCompra.frecuencia').addClass('Inactivo');
  }
  SalesUp.Variables.CalcularLicenciaSalesUp();
}
SalesUp.Variables.ValidarNoNegativos=function(Op){
  var valor=(Op.v)?Op.v:1;
  if(valor<0){
    valor=-1*valor;
   $('#NumUsuarios').val(valor);
  }
  return valor;
}


SalesUp.Variables.CalcularLicenciaSalesUp=function(){
    var idLicencia=$('#idLicencia').val();
    var tipoLicencia=$('#tipoLicencia').val();
    var maxUsuarios=$('#MaxUsuarios').val();
    var NumUsuarios=$('#NumUsuarios').val();
    var paisDef=$('#paisDefault').val();
    var diasPagar=$('#diasapagar').val();
    var frecuencia=$('.frecuencialicencia:checked').val();
    var desc=(frecuencia==3)?0:(frecuencia==6)?10:(frecuencia==12)?20:0;
    var version=$('.licencias:checked');
    var precioPro=$('#Licencia2');
    var precioBasico=$('#Licencia1');
    var pPro=(paisDef=='MX')? $(precioPro).attr('data-preciomx'):$(precioPro).attr('data-preciousd');
    var pBasico=(paisDef=='MX')? $(precioBasico).attr('data-preciomx') : $(precioBasico).attr('data-preciousd');
    var desc=(frecuencia==3)?0: (frecuencia==6)? 10: (frecuencia==12)?20:0;
    var LicActual=$('.licencias:checked').val();
    var precio=(idLicencia ==1) ? pBasico: pPro;
    var precioPromo=(precio-(precio*desc/100));
    var tipo='R';
    
        precioBasico=(frecuencia==3)? $('#BasicaTrim').val() : (frecuencia==6) ? $('#BasicaSem').val() : (frecuencia==12) ? $('#BasicaAnual').val() :'';
        precioPro=(frecuencia==3)? $('#ProTrim').val() : (frecuencia==6) ? $('#ProSem').val() : (frecuencia==12) ? $('#ProAnual').val()  :'';

        precio=(idLicencia ==1) ? precioBasico: precioPro;

    var IVA=16;
    var Subtotal=0.0;
    $('.ActualizarVersion').css('display', 'block');
    var tipoOperacion=$('.tipooperacion:checked').val();
    if(tipoOperacion==0){
     if(idLicencia==LicActual){
        SalesUp.Variables.MostrarTextoPrecio({precio:precio}); //precio
        var opt={precio:precio, frec:frecuencia, desc:0, numUs:NumUsuarios};
        Subtotal=SalesUp.Carrito.RenovarMismaVersion(opt);

        $('#frm_u').val(NumUsuarios);
        tipo=='R';
      }else if((idLicencia==1) && (LicActual==2)){
        precio=precioPro;
        SalesUp.Variables.MostrarTextoPrecio({precio:precio});
        var opt={maxus:maxUsuarios,pBasico:pBasico, pPro:pPro, diaspag:diasPagar, frec:frecuencia, desc:0, numUs:NumUsuarios, prepromo:precio};
        Subtotal=SalesUp.Carrito.RenovarLicenciaBasicaAPro(opt);
        $('#frm_u').val(NumUsuarios);
        tipo=='R';
      }else if((idLicencia==2) && (LicActual==1)){
        precio=precioBasico;
        SalesUp.Variables.MostrarTextoPrecio({precio:precio});
        var opt={precio:precio, frec:frecuencia, desc:0, numUs:NumUsuarios};
        Subtotal=SalesUp.Carrito.RenovarMismaVersion(opt);
        $('#frm_u').val(NumUsuarios);
        tipo=='R';
      }
    }else if(tipoOperacion==1){
      var precio=(LicActual==1)?precioBasico: precioPro;

      SalesUp.Variables.MostrarTextoPrecio({precio:precio});
      var opt={UsNuevos:NumUsuarios, precio:precio, diaspag:diasPagar};
      Subtotal=SalesUp.Carrito.AumentarUsuarios(opt);
      $('#frm_u').val(NumUsuarios);
      tipo=='u';
    }else if(tipoOperacion==2){

      var precio=(LicActual==1)?precioBasico: precioPro;
      SalesUp.Variables.MostrarTextoPrecio({precio:pPro});
      var opt={pPro:precioPro, pBasico:precioBasico, maxus:maxUsuarios, dias:diasPagar };
      Subtotal=SalesUp.Carrito.AmpliarBasicaAPro(opt);
      
      $('#frm_u').val(maxUsuarios);
      tipo=='A';
    }
    Impuesto=(paisDef=='MX')?(Subtotal*IVA)/100 : 0;
    Total=parseFloat(Subtotal)+parseFloat(Impuesto);
    Subtotal=parseFloat(Subtotal).toFixed(2);
    Impuesto=parseFloat(Impuesto).toFixed(2);
    Total=parseFloat(Total).toFixed(2);
    Subtotal=SalesUp.Sistema.moneda({numero:Subtotal}); 
    Impuesto=SalesUp.Sistema.moneda({numero:Impuesto}); 
    Total=SalesUp.Sistema.moneda({numero:Total}); 
    $('#Subtotal').html(Subtotal);
    $('#Impuestos').html(Impuesto);
    $('#Total').html(Total);
    var fp=$('.forma_pago:checked').val();
    $('#id_licencia').val(LicActual);
    $('#frm_f').val(frecuencia);
    $('#frm_s').val(Subtotal);
    $('#frm_d').val(0);
    $('#frm_i').val(Impuesto);
    if(tipoLicencia=='1'){
       $('#frm_t').val('S');
    }else {
      $('#frm_t').val(tipo);
    }
    var idmon=1;
    idmon=(paisDef!='MX')? 15:''; 
    $('#idmoneda').val();
    $('#frm_m').val(Total);
    $('#frm_m_s').val(Subtotal);
    $('#frm_p').val(0);
    $('#frm_c').val(0);
    $('#frm_c_m').val(0);
    $('#frm_t_f').val(fp);
    var tt=$('#TARJETA_TIPO').val();
    var nom=$('#TARJETA_NOMBRE').val();
    var tnum=$('#TARJETA_NUMERO').val();
    var tcod=$('#TARJETA_CODIGO').val();
    var tmes=$('#TARJETA_MES').val();
    var tanio=$('#TARJETA_ANNIO').val();
    $('#frm_t_t').val(tt);
    $('#frm_t_nom').val(nom);
    $('#frm_t_num').val(tnum);
    $('#frm_t_mes').val(tmes);
    $('#frm_t_a').val(tanio);
    $('#frm_t_c').val(tcod);
    $('#frm_t_boni').val(0);
    $('#frm_t_usado').val(0);
    $('#frm_t_bu').val(0);
    $('#frm_openpay_oculto').val($("#deviceIdHiddenFieldName").val());	
	

}

