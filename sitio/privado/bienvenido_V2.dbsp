	<!--script type="text/javascript" src="../scripts/basicos/popup.js"></script-->
	<script>
	
		 var Enviado = "Enviado";
			 var Video = "video"; 
			  var AlertaVideoTour = {
			     video : function(iframe){
			     	var gap     = 20;
					var boxH    = $(window).height() - gap;    
					var boxW    = $(window).width() - gap * 2;
					         $.fallr('show', {
			                      buttons : {
			                          button1 : {text: 'Cerrar'}
			                      },
			                      content : iframe,
			                       position: 'center',
			                       closeKey : true,
			                       width   : boxW,
    							   height  : boxH
			                  });
			            }
	};
	
	
	$(document).ready(function(){
		self.parent.CambiarFondo();
		CargarContenido("bienvenido_1_V2.dbsp","Existen las ventas y la pasión por las ventas","Pantalla1",1);
	});
	
		$(".Pantalla1").live("click",function(){
			CargarContenido("bienvenido_1_V2.dbsp","Existen las ventas y la pasión por las ventas","Pantalla1",1);
		});
		
		$(".Pantalla2").live("click",function(){
			CargarContenido("bienvenido_2_V2.dbsp","Proceso de ventas","Pantalla2",2);
		});
		
		$(".Pantalla3").live("click",function(){
			CargarContenido("bienvenido_3_V2.dbsp","Pantalla principal","Pantalla3",3);
		});
		
		$(".Pantalla4").live("click",function(){
			CargarContenido("bienvenido_4_V2.dbsp","Prospectos nuevos","Pantalla4",4);
		});
		
		$(".Pantalla5").live("click",function(){
			CargarContenido("bienvenido_5_V2.dbsp","Pantalla de oportunidades","Pantalla5",5);
		});
		
		$(".Pantalla6").live("click",function(){
			CargarContenido("bienvenido_6_V2.dbsp","Registrando ventas","Pantalla6",6);
		});

		$(".Pantalla7").live("click",function(){
			CargarContenido("bienvenido_7_V2.dbsp","Capacitación en línea","Pantalla7",7);
		});
		
		$(".Pantalla8").live("click",function(){
			CargarContenido("bienvenido_8_V2.dbsp","Antes de terminar te invitamos a nuestro  Video Tour","Pantalla8",8);
		});
		
		$(".video").live("click",function(){
			var frame = $(this).attr("rel");
			frame='<div id="loadingsalesup"><img src="../imagenes/loadingsalesup.png" /></div>'+frame;
			AlertaVideoTour[Video].apply(this,[frame]);
			setTimeout(function(){$("#fallr-icon").remove();},500);
			setTimeout(function(){ $('#loadingsalesup').remove();  }, 2000);
		});			
		
		$("#CerrarBienvenido").live("click",function(){
			if ($("#CerrarBienvenido").is(':checked')){$.post("ocultar-bienvenida.dbsp?ban=1");}
			else{$.post("ocultar-bienvenida.dbsp?ban=0");}
		});
		
		$("#ArrowLeft, #ArrowLeftVideo").live("click",function(){
			$('#Prev').click();
		});
		
		$("#ArrowRight, #ArrowRightVideo").live("click",function(){
			$('#Next').click();
		});
		
		
		$(".Cerrar").live("click",function(){
			self.parent.tb_remove();
		});
				
		function CargarContenido (pagina,titulo,id,inicia){
			var direccion = '/privado/ajax/'+pagina;
			$.ajaxSetup({  'beforeSend' : function(xhr) {  xhr.overrideMimeType('text/html; charset=iso-8859-1'); } });
			
			$.ajax({ async : false, cache : false, dataType : 'html',
					url : direccion,
					success : function(data) {
						$('#CargaBienvenido').html(data);
						$("#BienvenidoTop h1").html(titulo);
						$('button, input:submit', 'form').button();
						$("img."+id).addClass("seleccionado");
						var a = $('#CargaBienvenido').find('#Next').length;
						if( a == 1 ){	
						$('#BienvenidoLeft').append('<div id="ArrowRight"></div>');
						/*	if( (inicia==1) || (inicia==7) ){  $('#BienvenidoLeft').append('<div id="ArrowRight"></div>'); 	}
							else{	$('#BienvenidoLeft').append('<div id="ArrowRightVideo"></div>'); } */
						}
						
						var a = $('#CargaBienvenido').find('#Prev').length;
						if( a == 1 ){
						    $('#BienvenidoLeft').append('<div id="ArrowLeft"></div>'); 
							 /*if( (inicia==1) || (inicia==7) ){  $('#BienvenidoLeft').append('<div id="ArrowLeft"></div>'); }
							else{	$('#BienvenidoLeft').append('<div id="ArrowLeftVideo"></div>'); }	 */
						}
						
						
						
						
						$('#BienvenidoBottom').append('<span id="nPaginas"> '+inicia+' / 7 </span>');
						
					setTimeout(function(){$('#loadingsalesup').remove();}, 1800);
					}
				}); //fin ajax
		}
	</script>
