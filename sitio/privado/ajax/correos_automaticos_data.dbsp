<#IF EXPRESSION="SESSION.MAILCONFIG=1">

    <#IF EXPRESSION="SESSION.CREARCOMUNICACIONES=2">
    <div class="acciones visualizar">
        <a class="thickbox boton mas" title="Crear campaña" id="AgregarTitulo" href="popup_correo_automatico_agregar.dbsp?TB_callback=DeseaAgregarPieza&keepThis=false&TB_iframe=true&height=190&width=450" >Agregar campaña.</a>
    </div>
    <#/IF>


<#ELSE>
  <#IF EXPRESSION="SESSION.CREARCOMUNICACIONES=2">
    <div class="acciones visualizar">
        <a class="boton mas" title="Crear campaña" id="Noconfigurado" href="#" >Agregar campaña.</a>
