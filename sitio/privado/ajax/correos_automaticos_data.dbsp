            <!-- <a class="thickbox editar" title="Editar campaña"  href="popup_correo_automatico_editar.dbsp?tkauto=<#tkauto/>&TB_callback=GetData&keepThis=false&TB_iframe=true&height=190&width=450" ></a>  -->
            
                <#IF EXPRESSION="ESTADO!=0"> 
                <span class="OpcionAcciones Pointer" href="#" onclick="SalesUp.Variables.pausar('<#tkauto/>')">
                        <i class="fa fa-lg fa-pause"></i> Desactivar campaña
                </span>
