				self.parent.SalesUp.Sistema.CambiarTamanioPopUp({Alto:(360+tx),Ancho:750});	
			}else{
				self.parent.SalesUp.Sistema.CambiarTamanioPopUp({Alto:640,Ancho:750});
			}

			Recalcula_Montos_Comisiones();
			Recalcula_Fechas();
		}/*MuestraPagos*/

		function Revisa_Errores(){
			var x = parseInt (document.frmVenta.noexhibiciones.value) +1;
			var Total=0;
			for (i=1; i<x; i++) {
				Valor = SalesUp.Sistema.quitarFormatoNumero($('#pago'+i).val());
				if (Valor>''){
					elMonto = parseFloat (Valor)	
				}else{
					elMonto = 0;	
				}
				Total+=elMonto;
			}
			
			var monto = SalesUp.Sistema.quitarFormatoNumero(document.frmVenta.monto.value);
			monto = monto.toFixed(2);
			if ( Total.toFixed(2) != monto){
				SalesUp.Construye.MuestraMsj({tMsg:4, Destino:'body', Msg:'La suma de los pagos no coincide con el monto total' }); 
				Ok = 0; 
			}else {         
				Ok = 1;
			}
		}/*Revisa_Errores*/

		 
		function Recalcula_Montos_Comisiones() {
			var elmontoparcial=0;
			$('.corto1a').attr('readonly','readonly');
			var x = SalesUp.Sistema.quitarFormatoNumero(document.frmVenta.noexhibiciones.value);
			x = parseInt (x)+1;

			var Com_modo = SalesUp.Sistema.quitarFormatoNumero(document.frmVenta.comision_modo.value);
			Com_modo = parseInt (Com_modo);

			var MontoTotal = SalesUp.Sistema.quitarFormatoNumero(document.frmVenta.monto.value);
		   
			/* --------- Montos ----------------- */

			/* Sumar los fijos....*/
			var MontoFijo=0;
			for (i=1; i<=NumPagosGobal; i++){
				if (Fijos[i-1]==1){MontoFijo+= SalesUp.Sistema.quitarFormatoNumero(document.getElementById('pago'+i).value);}
			}

			/* Calcula los subsecuentes...*/
			if (MontoFijo<MontoTotal){
				var PagoSubsecuente = roundNumber((MontoTotal - MontoFijo) / (x - 1- NoFijos()),2);
			}else{var PagoSubsecuente = 0}   

			/* Cambia los montos subsecuentes...*/
			for (i=1; i<=NumPagosGobal; i++){
				var $pago = $('#pago'+i);
				if (Fijos[i-1]==0) {
					if (i<x){
						var new_pago = PagoSubsecuente;
						$pago.val(SalesUp.Sistema.formatoNumero(new_pago));
						elmontoparcial+= SalesUp.Sistema.quitarFormatoNumero($pago.val());
					}else{
						$pago.val(SalesUp.Sistema.formatoNumero(0));
					}
				}
			}

			if(MontoFijo<MontoTotal){
				var p1 = SalesUp.Sistema.quitarFormatoNumero($('#pago1').val());
				$('#pago1').val(SalesUp.Sistema.formatoNumero(p1+MontoTotal-MontoFijo-elmontoparcial));
			}
			/* ----------- Comisiones ------------ */
		   
			pct = SalesUp.Sistema.quitarFormatoNumero($('#comision_pct').val());
			for(i=1; i<=NumPagosGobal; i++){
				if (Com_modo==0){
					//comsiones prorateadas   
					pago = SalesUp.Sistema.quitarFormatoNumero($('#pago'+i).val());   
					var new_com=pago * pct / 100;          
					$('#comision'+i).val(SalesUp.Sistema.formatoNumero(new_com));
				}else{
					if(Com_modo==1){
						// una solaexhibiciÃƒÂ³n - primer pago	 
						if (i==1){
							var com_unic=$('#comision_monto').val();     
							$('#comision'+i).val(SalesUp.Sistema.formatoNumero(com_unic));
						}else{
							$('#comision'+i).val(SalesUp.Sistema.formatoNumero(0));
						}
					}else{
						if (Com_modo==2){
							// una solaexhibiciÃƒÂ³n -- ultimo pago
							if (i==(x-1)){
							var com_unic=$('#comision_monto').val();     
							$('#comision'+i).val(SalesUp.Sistema.formatoNumero(com_unic));
							}else{
								$('#comision'+i).val(SalesUp.Sistema.formatoNumero(0));
							}
						}else{
							if (Com_modo==3){
								// manual
								$('.corto1a').removeAttr('readonly');
							}
						}
					}

				}
			}
		}/*Recalcula_Montos_Comisiones*/
		 
		 
		function Recalcula_Fechas() {
			var x = parseInt (document.frmVenta.noexhibiciones.value) +1;
			var Com_modo = parseInt (document.frmVenta.comision_modo.value);
			var periodicidad =parseInt (document.frmVenta.periodicidad.value);
			
			switch (document.frmVenta.periodicidad.value){
				case '1': var Im =1 ; break;
				case '2': var Im =2 ; break;
				case '3': var Im =3 ; break;
				case '4': var Im =6 ; break;
				case '5': var Im =12 ; break;
				case '7': var Im =7 ; break;
				case '8': var Im =14 ; break;
				default :  Im=0;	 break	   
			}

			if (Im>0) {
				for (i=2; i<=NumPagosGobal; i++) {
					FS = new DateSql(document.getElementById('fecha1').value,convertcode);
					if (periodicidad==1 || periodicidad==2 || periodicidad==3 || periodicidad==4 || periodicidad==5){
						document.getElementById('fecha'+i).value=FS.add(Im*(i-1)).months().format(convertcode);
					}
					if (periodicidad==7 || periodicidad==8){
						document.getElementById('fecha'+i).value=FS.add(Im*(i-1)).days().format(convertcode);
					}
				}
			}
		}/*Recalcula_Fechas*/
		 
		  
		/* ------------- Cambios de controles -----------------------------*/
		 
		function Cambia_Pago(i) {
		   //EstandarizaNumero (document.getElementById('pago'+i));
			pago = SalesUp.Sistema.quitarFormatoNumero($('#pago'+i).val()); 

			pct  = SalesUp.Sistema.quitarFormatoNumero($('#comision_pct').val());

			NuevaComision = pago * pct / 100;           
			$('#comision'+i).val(SalesUp.Sistema.formatoNumero(NuevaComision));
			Fijos[i-1] = 1;

			Recalcula_Montos_Comisiones();

			Revisa_Errores()
		}/*Cambia_Pago*/
		 
		 
		function Cambia_No_Pagos() {
			Limpia_Fijos();
			MuestraPagos();
			Revisa_Errores();
		}
		 
		function Cambia_Periodicidad() {
			Recalcula_Fechas();
			Revisa_Errores();
		} 
		 
		function Cambia_Tipo_Comisiones() {
			Recalcula_Montos_Comisiones();
			Revisa_Errores();
		}

		function Cambia_Fecha_Cierre() {
			document.frmVenta.fecha1.value = document.frmVenta.fecha_cierre.value;
			Recalcula_Fechas();  
			Revisa_Errores();
