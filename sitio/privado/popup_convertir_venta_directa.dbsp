		function Cambia_Monto_Total() {
			
			if (document.frmVenta.monto.value==''){document.frmVenta.monto.value =SalesUp.Sistema.formatoNumero(0);}
			 
			var Tot = SalesUp.Sistema.quitarFormatoNumero(document.frmVenta.monto.value);
			var Pct = SalesUp.Sistema.quitarFormatoNumero(document.frmVenta.comision_pct.value);
			var tmp=Tot*(Pct/100);
			document.frmVenta.comision_monto.value= SalesUp.Sistema.formatoNumero(tmp);
			Limpia_Fijos();
			Recalcula_Montos_Comisiones();
		}/*Cambia_Monto_Total*/


		function Cambia_Monto_Total_Pct() {
			var tiene_productos = $('#tProductos').val();

			if(tiene_productos ==  0 ){
				$('#comision_pct').removeAttr('onfocus').removeClass('disabled');
				if(document.frmVenta.monto.value>''){
					if (document.frmVenta.comision_monto.value==''){
						var monto_pesos = 0;
						var comision_pct = 0;
						document.frmVenta.comision_monto.value = SalesUp.Sistema.formatoNumero(0);
					}else{
						var monto_pesos =SalesUp.Sistema.quitarFormatoNumero($('#monto').val());
						var comision_pct = SalesUp.Sistema.quitarFormatoNumero($('#comision_pct').val());

						if (document.frmVenta.comision_pct.value==''){
							document.frmVenta.comision_pct.value=SalesUp.Sistema.formatoNumero(0); 
							var comision_pct=0;
						}else{

							var comision_pct = parseFloat (comision_pct)/100;

						}  
					}
					var tmp=monto_pesos*comision_pct;
					$('#comision_monto').val(SalesUp.Sistema.formatoNumero(tmp));
				}
			}else{
				$('#comision_pct').attr('onfocus','this.blur();').addClass('disabled');
				Calcula_Porcentaje();	
			}    	

		   Limpia_Fijos();
		   Recalcula_Montos_Comisiones();
		}/*Cambia_Monto_Total_Pct*/
		
		function Calcula_Porcentaje(){
		  if (document.frmVenta.monto.value>''){ 
			var monto=0;
			var comision_monto=0;
			comision_monto =SalesUp.Sistema.quitarFormatoNumero($('#comision_monto').val());
			monto= SalesUp.Sistema.quitarFormatoNumero($('#monto').val());
			if (monto==0 || monto=='NaN'){
			  var tmp=0;
			}else{
			  var tmp=((comision_monto/monto)*100);
			}
			$('#comision_pct').val(SalesUp.Sistema.formatoNumero(tmp));
			}
		}/*Calcula_Porcentaje()*/

		function Cambia_Comision_Total() {

			var Monto = SalesUp.Sistema.quitarFormatoNumero(document.frmVenta.monto.value);

			if (document.frmVenta.comision_monto.value==''){ document.frmVenta.comision_monto.value = SalesUp.Sistema.formatoNumero(0); }

			var Comision = SalesUp.Sistema.quitarFormatoNumero(document.frmVenta.comision_monto.value);

			var pct= Comision / Monto*100;

			if ((pct=='NaN')||(pct=='Infinity')||(pct=='')){ pct=0; }

			document.frmVenta.comision_pct.value = SalesUp.Sistema.formatoNumero(pct);

			Recalcula_Montos_Comisiones();
		}/*Cambia_Comision_Total*/

		function MuestraPagosSinCrecer(){
			Resetea_pagado_cero();
			var x = parseInt (document.frmVenta.noexhibiciones.value) +1;
			var num=document.frmVenta.noexhibiciones.value; 
			
			for(i=1; i<=NumPagosGobal; i++){
				if ( i < x ){
					document.getElementById('p'+i).style.display='block';
				}else{
					document.getElementById('p'+i).style.display='none';
