			Link:'/privado/Modelo/jsonDatosProspecto.dbsp', 
			Parametros: 'idp='+Idp+'&tkp='+SalesUp.Variables.Tkp, 
			DataType:'json' 
		  });
		  jResp = jResp.jsonDatos;
		  SalesUp.Variables.tkcom = jResp[0].TKCOM;
		  if (jResp[0].TKCOM != null) {
		  	var jRespComp = SalesUp.Sistema.CargaDatos({ 
		  		Link:'/privado/Modelo/jsonInformacionEmpresa.dbsp', 
		  		Parametros: 'tkcom='+jResp[0].TKCOM, 
		  		DataType:'json' 
		  	});
		  	setTimeout(function(){
				for (var i = 1; i <= 10; i++) {
					var CCAMPO = 'Campo'+i+'C';
					var dsto = jRespComp.jsonDatos[0][CCAMPO];

					var $inpt = $('[name="Campo'+i+'C"]');
					$inpt.val(dsto);
				}
		  	},100)
			
		  }else{
			$('.TbEmpresas').parent().remove();
		  }
		  //Variable global para saber si el prospecto fue canalizado

		  SalesUp.Variables.OpcionMostrar = jResp[0].OpcionMostrar;

		  for(var xcp=0;xcp<_.size(jCampoClientes);xcp++){
			var jcp = jCampoClientes[xcp], $cp = $('#'+jcp.attr_id), indice = jcp.attr_data_Indice, vcp = jResp[0]['Campo'+indice];
			$cp.val(vcp);
		  }

		  SalesUp.Variables.llenaControles();

		  $('.decimal').attr('onKeyPress','return SalesUp.Variables.valDecimales({t:this, e:event});').attr('onblur','SalesUp.Variables.numerosDecimales({t:this});');
		  $('.numero').attr('onKeyPress','return SalesUp.Variables.valNumero({t:this, e:event});').attr('onblur','SalesUp.Variables.numerosEnteros({t:this});');

		  AgregaFuncionesCamposSugeridos();
		  onblurInfoUnico();

		}/*SalesUp.Variables.camposPersonalizablesClientesVentas*/

		SalesUp.Variables.camposPersonalizablesClientesVentas();

		$("#comision_pct").live('change',function(){ Cambia_Monto_Total_Pct(); });

		var NumPagosGobal=96;
