			Parametros:'idventana=3', 
			DataType:'json'
		  });

		  var jCampoVentas = SalesUp.Sistema.CargaDatos({
			Link:'/privado/Modelo/jsonCampos.dbsp', 
			Parametros:'idventana=4', 
			DataType:'json'
		  });

