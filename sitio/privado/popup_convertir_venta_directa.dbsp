		  jAux2 = jAux;
		  for(var xx=0;xx<_.size(jAux);xx++){
			var cant=0;
			 for(var yy=0;yy<_.size(jAux2);yy++){
				if(jAux[xx].IdCampo == jAux2[yy].IdCampo){  		
					if(cant==1){
						jAux2.splice(yy,1);
						jAux=jAux2;
						xx=0;
						break;
					}else{
						cant=1;
					}
				}
			}
		  } 
		

		  SalesUp.Variables.jsonCampos = jAux;
		  var infoJson = jAux;
		  for (var xy = 0; xy < _.size(infoJson); xy++){
			var j = infoJson[xy];
			var Seleccione = {};
			Seleccione.value = '';
			Seleccione.Opcion = '(... Seleccione una opciÃƒÂ³n ...)';

			(j.attr_maxLength=='0') ? j.attr_maxLength='':'';

			if(j.esSelect == '1'){
