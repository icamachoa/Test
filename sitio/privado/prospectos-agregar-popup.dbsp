          //+++++++++++++++++++++++++++++++++++++++++++++
     
	  	function ValidaFecha(f){
		  	return (f).match(/([0-9]{2})\/([0-9]{2})\/([0-9]{4})/);
	  	}
	  	
	  	function RevisaFecha(t,v){
	  		var b = true;
		  	if(v!=''){
			    if(!ValidaFecha(v)){
			    	$(t).addClass("mal");
			    	$('#warninginfo').html('');
					$('#warninginfo').stop().html('Formato de fecha invalido <strong>dd/mm/aaaa</strong>').show('slow').delay(3000).hide('slow');
					b = false;
			    }
			}
			return b;
	  	}
 
		$(".fecha").blur(function(){
			RevisaFecha($(this),$(this).val());
		});
		
		function RecorreFechas(){
			
			var b2 = true;
			$('.fecha').each(function() {
				
				var v = $(this).val();
			  	if(b2){
				  	if(v!=''){
					    if(!ValidaFecha(v)){
					    	$(this).addClass("mal");
					    	$('#warninginfo').html('');
							$('#warninginfo').stop().html('Formato de fecha invalido <strong>dd/mm/aaaa</strong>').show('slow').delay(3000).hide('slow');
							b2 = false;
					    }
					}
			  	}
				
			});
			return b2;
		}
     
     //+++++++++++++++++++++++++++++++++++++++++++++
     
     
     
     
    	function Mytrim(myString) {
			return myString.replace(/^\s+/g, '').replace(/\s+$/g, '')
		}

		function ValidateInteger(e) {
			var keynum, keychar, numcheck;

			if(window.event) {// IE
				keynum = e.keyCode;
			} else if(e.which) {// Firefox/Opera
				keynum = e.which;
			}
			keychar = String.fromCharCode(keynum);
			numcheck = /\d/;
			return numcheck.test(keychar);
		}

		function ValidaAdicionales(){
			
			setTimeout(function(){
        $('.adicional').each(function() {
          $('#' + $(this).attr('id') + '_val').val($(this).val());
        });
        var Pasa = ValidaObligatorios();

        if(!Pasa){
          setTimeout(function(){ $('#Overlay').remove(); },50);
        }else{
          $('#frmProspecto').submit();
        }
      },200);

		}

		function ValidaObligatorios() {
			
			var flag = true;
			$('.obligatorio').each(function(){
				if($(this).val() == '' || $(this).val() == null) {
					$(this).addClass("mal");
					flag = false;
				}
			});
			
			if(flag==false){
				$('#warninginfo').html('');
				$('#warninginfo').stop().html('Los campos marcados son <strong>obligatorios</strong>').show('slow');
				$('#warninginfo').delay(3000).hide('slow');
				$("input.mal:first").focus();
			}
			
			if(flag) flag = RecorreFechas();
			
			if(flag) flag = ValidaCamposLlave();
		
			return flag;
		}

		function ValidaCamposLlave() {
		
			var flag = true;
				$('.llave').each(function() {
						flag = ((flag) && (ValidaLlave($(this).attr('rel'))));
				});
			return flag;
		}
		
		function ValidaLlave(id){
					
					var resultado = true;
					var idcampo = $('#campo_llave' + id).val();
					var valor, nombre, res , nombreUsr , Descartado , txtDescartado;
					
					if(idcampo == '-6') {//es correo
						valor = $('#campocon-6').val();
						nombre = $('label[for=campocon-6]').html();
					} else {// es adicional
						if (id>0){
							valor = $('#campo' + id).val();
							nombre = $('label[for=campo' + id + ']').html();	
						}else{
							valor = $('#campocon' + id).val();
							nombre = $('label[for=campocon' + id + ']').html();
						}
					}
				
					
					valor = Mytrim(valor);
					$('#warning').html('<img src="../imagenes/loadingIconSmall.gif"/> Validando...').show();
					
          $("#btnAceptar").attr("disabled", "true");
					$("#btnAceptaryVer").attr("disabled", "true");

          $("#btnAceptar").addClass("ui-button-disabled ui-state-disabled");
					$("#btnAceptaryVer").addClass("ui-button-disabled ui-state-disabled");
					
					
					if(valor != ''){
		       

          	$.ajax({ async:false, cache:false, dataType:'html',
							url : '/privado/ajax/obtiene_validaciones_paso_1.dbsp?idprospecto=' + idprospectoactual + '&idcampo=' + idcampo + '&valor=' + valor,
							success: function(data){
								$("#DivResultado").html(data);
								res = $('#ResultadoValida').val();
								Descartado = $('#ResultadoDescartado').val();
								if (Descartado==1){txtDescartado=' <b style="color:red;">(Descartado)</b> '}else{txtDescartado=''}
								$.ajax({ async: false, cache: false, dataType: 'html',
									url: '/privado/ajax/obtiene_validaciones_paso_2.dbsp?idprospecto=' + idprospectoactual + '&idcampo=' + idcampo + '&valor=' + valor,
									success : function(data) {
										$("#DivResultado2").html(data);
										nombreUsr = $('#PerteneceUsuario').val();
										
										if(res < 0) {
											$('#campocon-6').addClass("mal");
											$('#warninginfo').html('');
											$('#warninginfo').html('El correo es inválido, por favor verifique de nuevo.').show('slow');
											$('#warninginfo').delay(3000).hide('slow');
											$('#campocon-6').focus();
											resultado = false;
										}
		
										if(res != 0) {
											$('#warning').html('El prospecto <b>' + res + '</b>'+txtDescartado+' asignado a <b>' + nombreUsr + '</b> fue capturado con el mismo <b>' + nombre + '</b>. Por favor revise la información.').show('slow');
											if (id>0){ 
													$('#campo' + id).addClass("mal");
													$('#campo' + id).focus();													 
												}else{ 
													$('#campocon' + id).addClass("mal");
													$('#campocon' + id).focus(); 
												}
											resultado = false;
										} else {
											resultado = true;
										}
		
										if(res == -1){
											$('#warning').html('').hide();
										}
									} /* /success */
								}); /* /Ajax  obtiene_validaciones_paso_2.dbsp */
		
							}/* /success */
						}); /* /Ajax  obtiene_validaciones_paso_1.dbsp */
		

          } /* /(valor != '') */


					$("#btnAceptar").removeAttr("disabled");
          $("#btnAceptaryVer").removeAttr("disabled");

					$("#btnAceptar").removeClass("ui-button-disabled ui-state-disabled");
					$("#btnAceptaryVer").removeClass("ui-button-disabled ui-state-disabled");
	
					return resultado;
	
				} /* /ValidaLlave */
				
				
				
				
				

				$(document).ready(function() {

					$('#nombre').keyup(function(){$('#nombre').attr('rel',1);});
					$("#campocon-5").change(function(){
						$("#campocon-5").removeClass('remarch').removeClass('remarcm');
						$('#sexoseleccion').attr('sel',1);
					});
					$('#nombre').focusout(function(){
						var BuscarNombre=$('#nombre').val();
						var rel=$('#nombre').attr('rel');
						var sel=$('#sexoseleccion').attr('sel');
						if((rel==1)&&(sel==0)){
						$.ajax({async:false,	cache: false,	dataType: 'html',
							url : 'ajax/jx-obten-sexo.dbsp?buscarnombre='+BuscarNombre,
							success : function(data) {
								$("#resultsexo").html(data);
							}
						});
						var resultsexo=$('#sexoprospecto').val();		
						if (resultsexo=='H'){
							$("#campocon-5").removeClass('remarch').removeClass('remarcm');
							$('#campocon-5').addClass('remarch');
							setTimeout(function(){$('#campocon-5').removeClass('remarch');},5000);
						}
						if (resultsexo=='M'){
							$("#campocon-5").removeClass('remarch').removeClass('remarcm');
							$("#campocon-5").addClass('remarcm');
							setTimeout(function(){$('#campocon-5').removeClass('remarcm');},5000);
						}
						$("#campocon-5 option[value="+resultsexo+"]").attr("selected","selected") ;
						$('#nombre').attr('rel',0);
						}else{$("#campocon-5").removeClass('remarch').removeClass('remarcm');}	
					});
					
					$('button, input:submit', 'form').button();

					$('input.fecha').datepicker(ConfiguracionPicker);

					setTimeout(function() {	$('.primercampo').focus();	}, 20);
					
					$('input, select, textarea').focus(function(){
						$(this).addClass('actual');
						$('label[for=' + $(this).attr('id') + ']').addClass('actual');
					}).blur(function() {
						$(this).removeClass('actual');
						$('label[for=' + $(this).attr('id') + ']').removeClass('actual');
					});

					$('input.naturalNumber').keyup(function() {
						this.value = this.value.replace(/[^0-9]/g, '');
					});
					
					$('input.naturalNumber').blur(function() {
						this.value = this.value.replace(/[^0-9]/g, '');
					});

					$('input.decimalNumber').keyup(function() {
						this.value = this.value.replace(/[^0-9\.]/g, '');
					});
					
					$('input.decimalNumber').blur(function() {
						this.value = this.value.replace(/[^0-9\.]/g, '');
					});

					$('input.numero').keyup(function() {
						this.value = this.value.replace(/[^0-9]/g, '');
					});
					
					$('input.numero').blur(function() {
						this.value = this.value.replace(/[^0-9]/g, '');
					});

					$('input.decimal').keyup(function() {
						this.value = this.value.replace(/[^0-9\.]/g, '');
					});
					
					$('input.decimal').blur(function() {
						this.value = this.value.replace(/[^0-9\.]/g, '');
					});

					$('.llave').keyup(function() {
						$('#warning').hide('slow');
						$(this).siblings('.errorValidacion').remove();
					});
					
					$('input').keyup(function() {
						$(this).removeClass("mal");
					});
					
					$("#btnAceptaryVer").click(function(){
						$("#ver").val(1);
						ValidaAdicionales();
					});
		      
          $('#idpais').change(function(){
            var fechaRandom = new Date();
            $.ajaxSetup({ 'beforeSend' : function(xhr) { xhr.overrideMimeType('text/html; charset=iso-8859-1'); } });
            $.ajax({async:false, cache: false, dataType: 'html',
              url : 'ajax/recargaestado-prospecto.dbsp?fecharandom='+fechaRandom.getTime()+'&IdPais='+$(this).val(),
              success : function(data) {
                $("#EstadoPais").html(data);
              }
            });
          });	

          SalesUp.Sistema.MuestraEspera('',4);
          $('#Overlay').remove();
          $('form').submit(function(){ SalesUp.Sistema.MuestraEspera('',4); });
					
				});	/* /fin ready */
    </script>
    
  </body>
